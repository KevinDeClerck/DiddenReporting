<?xml version='1.0' encoding='utf-8'?>
<widget id="be.odisee.kevindeclerck.mi3project1" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mi3Project1</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="kevin.declerck96@gmail.com" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png" />
    <preference name="StatusBarOverlaysWebView" value="false" />
</widget>
